<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.1" >
    <persistence-unit name="my-persistence-unit">
        <class>com.example.mainApp.Entity.Godziny</class>
        <class>com.example.mainApp.Entity.Pokoje</class>
        <class>com.example.mainApp.Entity.Rezerwacje</class>
        <class>com.example.mainApp.Entity.Uzytkownicy</class>
        <class>com.example.mainApp.Entity.Wiadomosci</class>
        <properties>
            <property name="hibernate.connection.url"
                      value="jdbc:postgresql://localhost/"/>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
            <property name="hibernate.connection.username" value="***"/>
            <property name="hibernate.connection.password"
                      value="***"/>
        </properties>
    </persistence-unit>
</persistence>
